; MOV32RI EBX x00
; MOV32RI ECX :BUFFER
; MOV32RI EDX 2

STRLEN:                 ; strlen(eax) -> eax
    MOV32RR EDX EAX
    STRLEN_LOOP:
        LOAD8RR EBX EAX
        CMP8RI EBX 0
        ADD32RI EAX 1
        JNZ32I :STRLEN_LOOP
        SUB32RR EAX EDX
        JMP32R ECX

LOGIN_MESSAGE:
$ "Type your password: " x0A x00
BUFFER:
$ x0000