START:
    MOV32RI EAX :LOGIN_MESSAGE
    MOV32RI ECX :CONTINUE
    JMP32I :STRLEN

CONTINUE:
    MOV32RR EDX EAX
    MOV32RI EAX 1
    MOV32RI EBX 1
    MOV32RI ECX :LOGIN_MESSAGE

SYSCALL
$ xFFFF ; Crash the program
; 
